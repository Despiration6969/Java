<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ProfileController">
   <children>
      <BorderPane fx:id="profileBorderPane" prefHeight="140.0" prefWidth="900.0">
         <center>
            <Label fx:id="profileTitle" alignment="CENTER" prefHeight="62.0" prefWidth="751.0" text="PROFILE AND MANAGE BORROWING" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font size="42.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
      <Pane fx:id="profilePane" prefHeight="517.0" prefWidth="900.0">
         <children>
            <Label fx:id="profileLabel2" alignment="CENTER_RIGHT" layoutX="225.0" layoutY="187.0" prefHeight="50.0" prefWidth="80.0" text="DOB: ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="profileTF1" editable="false" layoutX="311.0" layoutY="128.0" prefHeight="50.0" prefWidth="365.0" />
            <TextField fx:id="profileTF2" editable="false" layoutX="311.0" layoutY="187.0" prefHeight="50.0" prefWidth="365.0" />
            <Label fx:id="profileLabel1" alignment="CENTER_RIGHT" contentDisplay="CENTER" layoutX="250.0" layoutY="128.0" prefHeight="50.0" prefWidth="60.0" text="Name: ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnProfileBack" layoutX="561.0" layoutY="312.0" mnemonicParsing="false" onAction="#btnBackPressed" prefHeight="50.0" prefWidth="115.0" text="Back" />
            <Label fx:id="profileLabel3" alignment="CENTER_RIGHT" layoutX="225.0" layoutY="248.0" prefHeight="50.0" prefWidth="80.0" text="Overdue: ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="profileTF3" editable="false" layoutX="311.0" layoutY="248.0" prefHeight="50.0" prefWidth="365.0" />
         </children>
      </Pane>
   </children>
</VBox>
